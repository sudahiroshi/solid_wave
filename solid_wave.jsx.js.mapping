<=9; k++ ) {\n\t\t\t\tvar yyy = -k + 5.1;\n\t\t\t\tp1.circle( xxx, -yyy, 1 );\n//\t\t\t\tx[j][k] = xxx;\n//\t\t\t\ty[j][k] = yyy;\n\t\t\t}\n\t\t}\n\t\tp1.fill();\n\n\t\tp1.beginPath();\n\t\tp1.forecolor( 255, 0, 0 );\n\t\tvar xx = p * mx2;\n\t\tvar xxx = mx2 + Math.cos( t - xx );\n\t\tfor( var k=1; k<=9; k++ ) {\n\t\t\tvar yyy = -k + 5.1;\n\t\t\tp1.circle( xxx, -yyy, 1 );\n\t\t}\n\t\tvar yyy = -5 + 5.1;\n\t\tfor( var j=0; j<=mx; j++ ) {\n\t\t\tvar xx = p * j;\n\t\t\tvar xxx = j + Math.cos( t- xx );\n\t\t\tp1.circle( xxx, -yyy, 1 );\n\t\t}\n\t\tp1.fill();\n\t\tthis.i++;\n\t}\n\n\tfunction animation2(): void {\n\t\tvar p2 = this.vc2;\n\t\t//var vy = 1.3;\n\t\t//var vyy = 0.63 * vy;\n\t\tvar mx = 30;\n\t\t//var mx2 = mx / 2;\n\t\t//var mx3 = mx2 + 0.4;\n\t\t//var my = mx2;\n\t\t//var mty = -my / 2;\n\t\tvar p = 2.0 * Math.PI / mx * 2;\n\t\tvar pb = 2.0 * Math.PI / 40;\n\t\tvar t = pb * this.i;\n\n\t\tp2.scale( mx * -0.004, -6, mx * 1.1, 12 );\n\t\tp2.beginPath();\n\t\tp2.forecolor( 120, 120, 120 );\n\t\tp2.cls();\n\t\tp2.line( mx * -0.01, 0, mx * 1.02, 0 );\n\t\tp2.line( mx * 1.77 / 3.45, -4.5, mx * 1.77 / 3.45, 4.4 );\n\t\tp2.stroke();\n\n\t\tp2.beginPath();\n\t\tp2.forecolor( 0, 0, 0 );\n\t\tfor( var j=0; j<=mx; j++ ) {\n\t\t\tfor( var k=2; k<=10; k++ ) {\n\t\t\t\tvar xx = p * j;\n\t\t\t\tvar xxx = j + 0.5;\n\t\t\t\tvar yyy = 1.5 * Math.sin( t - xx ) + k - 5.7;\n\t\t\t\tp2.circle( xxx, -yyy, 1 );\n\t\t\t}\n\t\t}\n\t\tp2.fill();\n\n\t\tthis.i++;\n\t}\n\n\t// 弾性表面波\n\tfunction animation3(): void {\n\t\tvar p3 = this.vc3;\n\t\tvar vy = 1.3;\n\t\tvar vyy = 0.63 * vy;\n\t\tvar mx = 30;\n\t\tvar mx2 = mx / 2;\n\t\tvar mx3 = mx2 + 0.4;\n\t\tvar my = mx2;\n\t\tvar myt = -my / 2;\n\t\tvar p = 2.0 * Math.PI / mx * 2;\n\t\tvar pb = 2.0 * Math.PI / 40;\n\t\tvar t = pb * this.i;\n\n\t\tp3.scale( mx * -0.004, myt / 1.1, mx * 1.17, my / 1.3 );\n\t\tp3.beginPath();\n\t\tp3.cls();\n\t\tp3.forecolor( 120, 120, 120 );\n\t\tp3.line( mx3 + 0.1, -5.8, mx3 + 0.1, 3.7 );\n\t\tp3.line( -1.1,